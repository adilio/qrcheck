{
  "name": "qrcheck",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:netlify": "netlify dev",
    "prebuild": "node scripts/prebuild.mjs",
    "build": "npm run prebuild && vite build && touch dist/.nojekyll",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .ts .",
    "test": "vitest run --reporter=verbose",
    "e2e": "playwright test",
    "ci:verify": "npm run typecheck && npm run lint && npm run test && npm run e2e && npm run build"
  },
  "netlify": {
    "dev": {
      "command": "npm run dev",
      "functions": "functions",
      "publish": "dist"
    }
  },
  "devDependencies": {
    "@netlify/functions": "^2.0.0",
    "@playwright/test": "^1.40.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "ajv": "^8.17.1",
    "esbuild": "^0.25.10",
    "eslint": "^8.50.0",
    "jsdom": "^23.0.0",
    "jsqr": "^1.4.0",
    "netlify-cli": "^23.9.1",
    "punycode": "^2.3.0",
    "svelte": "^4.2.0",
    "typescript": "^5.2.0",
    "vite": "^5.4.21",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "ajv-formats": "^3.0.1",
    "undici": "^7.16.0"
  }
}
